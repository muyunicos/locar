{
  "profile_data": {
    "name": "Peluquería Estilo Único",
    "address": "Calle Falsa 123",
    "phone": "555-1234",
    "favicon": "/assets/icons/default.ico",
    "banner": "/assets/banners/default.jpg"
  },
  "default_skin": "default",
  "timed_events": [
    {
      "name": "Navidad",
      "priority": 100,
      "when": {
        "date_range": { "start": "2024-12-01", "end": "2024-12-26" }
      },
      "then": {
        "set_skin": "christmas",
        "set_title_suffix": " - ¡Feliz Navidad!",
        "set_favicon": "/assets/icons/christmas.ico",
        "set_banner": "/assets/banners/christmas.jpg"
      }
    },
    {
      "name": "Happy Hour Semanal",
      "priority": 50,
      "when": {
        "days_of_week": [4, 5], 
        "time_range": { "start": "18:00", "end": "20:00" }
      },
      "then": {
        "set_skin": "happy_hour",
        "set_title_suffix": " - ¡Happy Hour!",
        "modify_module_data": {
          "catalog": { "action": "apply_discount", "value": 15 }
        }
      }
    },
    {
      "name": "Noche",
      "priority": 10,
      "when": {
        "time_range": { "start": "20:00", "end": "07:00" }
      },
      "then": {
        "set_skin": "dark"
      }
    }
  ],
  "modules": [
    { "type": "banner" },
    { "type": "profile", "data_key": "profile_data" },
    { "type": "catalog", "title": "Nuestros Productos", "data_file": "catalog_items.json" }
  ]
}